    customProperties.put("docType",        getInputParams("Document_type"));  
    customProperties.put("folderPath",     getInputParams("BRANCHID")+ '/' + getInputParams("USERACCOUNTID") + '/' + Sub_Product_ID + '/' +getInputParams("Loan_ID"));
